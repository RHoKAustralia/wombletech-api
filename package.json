{
  "name": "wombletech-api",
  "description": "Lambda hosted API created for Wombletech as part of RHoK'n'RHoL 2021",
  "private": true,
  "scripts": {
    "lint-fix": "npx prettier --write src/**/*.ts",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && NODE_ENV=development webpack",
    "build-prd": "yarn clean && NODE_ENV=production webpack",
    "deploy": "yarn build-prd && sam deploy"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/node": "^16.9.1",
    "@types/webpack": "^5.28.0",
    "aws-sdk": "^2.987.0",
    "glob": "^7.1.7",
    "prettier": "^2.4.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "uuid": "^8.3.2"
  }
}
